{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/images/**",
        "destination": "/images/"
      },
      {
        "source": "/designs/**",
        "destination": "/designs/"
      },
      {
        "source": "/#/**",
        "destination": "/index.html/#/"
      },
      {
        "source": "/app/hello", "function": "app"
      }
    ]
  },
  "functions": {
  }
}
