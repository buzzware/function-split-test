{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/front/**", "function": "front"
      },
      {
        "source": "/api/**", "function": "api"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
