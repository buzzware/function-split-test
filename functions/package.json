{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "version": "0.0.0",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Gary McGhee",
    "email": "gary@buzzware.com.au"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": ".",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "8"
  },
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "npm run eslint && NODE_ENV= npm run mocha",
    "eslint": "eslint ./. test/. --config .eslintrc.json",
    "dev": "nodemon ./",
    "start": "node ./",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@awaitjs/express": "=0.1.4",
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "=3.3.6",
    "@feathersjs/express": "=1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "firebase-admin": "=7.0.0",
    "firebase-functions": "=2.2.1",
    "helmet": "^3.16.0",
    "morgan": "=1.9.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.11",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "private": true
